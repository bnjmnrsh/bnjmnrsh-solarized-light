        "activityBarBadge.foreground": "#002B36",

        // badge
        "badge.background": "#fde001",
        "badge.foreground": "#002B36",

        // button
        "button.background": "#299df0",
        "button.foreground": "#f6f5f2",
        "button.hoverBackground": "#a09b8d",

        // contrast
        "contrastActiveBorder": "#fde001",
